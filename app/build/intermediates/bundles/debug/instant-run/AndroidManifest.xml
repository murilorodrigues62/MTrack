<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.rodrigues.murilo.mtrack"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Main" >
        <activity android:name="br.com.rodrigues.murilo.mtrack.activity.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.com.rodrigues.murilo.mtrack.activity.ListOrderActivity"
            android:label="Order List"
            android:parentActivityName="br.com.rodrigues.murilo.mtrack.activity.MainActivity" />
        <activity
            android:name="br.com.rodrigues.murilo.mtrack.activity.OrderActivity"
            android:label="Order"
            android:parentActivityName="br.com.rodrigues.murilo.mtrack.activity.MainActivity" />
        <activity
            android:name="br.com.rodrigues.murilo.mtrack.ui.quote.ArticleDetailActivity"
            android:label=""
            android:parentActivityName="br.com.rodrigues.murilo.mtrack.activity.MainActivity" />
        <activity
            android:name="br.com.rodrigues.murilo.mtrack.activity.SettingsActivity"
            android:label="Settings"
            android:parentActivityName="br.com.rodrigues.murilo.mtrack.activity.MainActivity" />
        <activity
            android:name="br.com.rodrigues.murilo.mtrack.activity.SyncActivity"
            android:label="Sync"
            android:parentActivityName="br.com.rodrigues.murilo.mtrack.activity.MainActivity" />
        <!-- android:parentActivityName=".ui.quote.ListActivity" /> -->

    </application>

</manifest>